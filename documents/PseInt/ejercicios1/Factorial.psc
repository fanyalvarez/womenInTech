//------Encontrar el factorial de cualquier número------
Algoritmo sin_titulo
	numero1<-0
	factorial<-1
	Escribir "ingresa el numero a calcular"
	Leer  numero1
	Repetir
		factorial <- factorial * numero1
		numero1 <- numero1 -1
	Hasta Que numero1 = 0 
	Escribir "su factorial es:", factorial
FinAlgoritmo








